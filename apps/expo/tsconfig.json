{
  "extends": ["@acme/tsconfig/base.json"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "jsx": "react-native",
    "checkJs": false,
    "moduleSuffixes": [".ios", ".android", ".native", ""]
  },
  "include": [
    ".expo/types/**/*.ts",
    "src",
    "app.config.ts",
    "eslint.config.mjs",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "reset.d.ts",
  ],
  "exclude": ["node_modules"]
}
